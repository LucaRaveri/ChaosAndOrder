<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="anchorPane"
            fx:controller="it.units.sdm.project.gui.controllers.SelectorSymbolController">

    <Label fx:id="nextMove" AnchorPane.leftAnchor="10.0"/>

    <RadioButton fx:id="crossRadio" onAction="#setEffect" AnchorPane.rightAnchor="40"/>
    <RadioButton fx:id="circleRadio" onAction="#setEffect" selected="true" AnchorPane.rightAnchor="120"/>

</AnchorPane>
