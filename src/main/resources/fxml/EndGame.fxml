<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sdm.project.guicomponents.controllers.EndGameController"
            prefHeight="230.0" prefWidth="500.0" stylesheets="/css/endgame.css">

    <Label fx:id="congratulations" AnchorPane.leftAnchor="50" AnchorPane.topAnchor="30"/>
    <ImageView fx:id="winnerImage" AnchorPane.rightAnchor="50" AnchorPane.topAnchor="20"/>

    <Button text="Back to board" fx:id="backToBoard" AnchorPane.leftAnchor="150" AnchorPane.bottomAnchor="20"/>
    <Button text="Exit Game" fx:id="exit"  AnchorPane.rightAnchor="150" AnchorPane.bottomAnchor="20"/>

</AnchorPane>
